<section class="hero">
    <div class="hero__inner-container">
        <p class="page-title"><span class="highlight">{{ shortTitle }}</span></p>
        <h1>Lead Generation</h1>
        <h4>We'll help you generate interest in your product and service.</h4>
        <ul class="flex-c space-between tag-container">
            {{#each tag}}
            <li class="flex-item btn small rounded outline">{{this}}</li>
            {{/each}}
        </ul> 
    </div>
</section>
<section>
    <div class="flex-c--col hero__inner-container">
    <ul class="flex-c flex-c--space-between label-row">
      <li class="label center lead">Landing pages</li>
      <li class="label center lead light">Websites</li>
      <li class="label center lead darker">Email</li>
      <li class="label center lead lighter">White papers</li>
      <li class="label center lead">Social media</li>
    </ul>
  </div>
</section>


